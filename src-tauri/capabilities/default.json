{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main",
    "settings"
  ],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:webview:allow-create-webview-window",
    "core:window:allow-close",
    "dialog:default",
    "dialog:allow-confirm"
  ]
}
